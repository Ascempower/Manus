{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "error",
    "prefer-const": "error",
    "no-var": "error",
    "eqeqeq": ["error", "always"],
    "curly": ["error", "all"],
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "off"
  }
}

